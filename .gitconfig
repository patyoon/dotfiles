[alias]
	co = checkout
	ci = commit
	rb = rebase
  amne = "ci -a "
	lol = log --oneline --graph --decorate
	stat = status
	ci = commit
	st = status
  sb = status -s -b
  ##### stash #####
	sa = stash apply
	sc = stash clear
	sd = stash drop
	sl = stash list
	sp = stash pop
	ss = stash save
  ##### merge #####
	m = merge
  ##### reset #####
  oops = reset --soft HEAD~1
	re = reset
	rh = reset HEAD
	reh = reset --hard
	rem = reset --mixed
	res = reset --soft
	rehh = reset --hard HEAD
	remh = reset --mixed HEAD
	resh = reset --soft HEAD
  #### rebase #####
  rb = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	rbs = rebase --skip
  #### push #####
	ps = push
	psf = push -f
	psu = push -u
	pso = push origin
	psfo = push -f origin
	psuo = push -u origin
	psom = push origin master
  save = stash save
  pop = stash pop --index
  gui = !sh -c '/usr/local/git/libexec/git-core/git-gui'
	mylog = "log --pretty=format: '%h %s [%an]\n\n' --graph"
	prune-empty="git filter-branch -f --prune-empty -- --all"
	amendn = "commit --amend --no-edit"
  rom = "rebase origin/master"
  slog = "log --decorate=short"
	d = difftool
  dc = diff --cached
	dk = diff --check
	dck = diff --cached --check
  ksdiff = difftool --gui
  cb = "!sh -c 'original_branch=$(git branch 2>/dev/null | grep '^*' | colrm 1 2); git co $0; git log | head -10; git co $original_branch'"
  del = "branch -D"
  save = stash save
  pop = stash pop --index
  restash = !git stash pop  --index &amp;&amp; git stash save
  rs = !git stash pop  --index &amp;&amp; git stash save
  s = status
  files = diff-tree -r --no-commit-id --name-only --diff-filter=ACMRT
  fixup = !sh -c 'git commit -m &quot;fixup! $(git log -1 --format='\''%s'\'' $@)&quot;' -
  squash = !sh -c 'git commit -m &quot;squash! $(git log -1 --format='\''%s'\'' $@)&quot;' -
  ri = rebase --interactive --autosquash
  f = fetch --all --tags --prune
  hist = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)&lt;%an&gt;%Creset' --abbrev-commit --date=relative
  rev = log -1 --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)&lt;%an&gt;%Creset'
## show the number of commits by each person.
  sh = shortlog -sn
  #############
	r = remote
	ra = remote add
	rm = remote rm
	rv = remote -v
	rp = remote prune
	rs = remote show
	rso = remote show origin
	rpo = remote prune origin
	#############
	w = show
	wp = show -p
	wr = show -p --no-color
	#############
	assume = update-index --assume-unchanged
	unassume = update-index --no-assume-unchanged
	assumed = "!git ls-files -v | grep ^h | cut -c 3-"
	snapshot = !git stash save "snapshot: $(date)" && git stash apply "stash@{0}"
	ours   = "!f() { git checkout --ours $@ && git add $@; }; f"
	theirs = "!f() { git checkout --theirs $@ && git add $@; }; f"
	human = name-rev --name-only --refs=refs/heads/*
[core]
	editor = "/Applications/Emacs.app/Contents/MacOS/Emacs"
	pager = "/usr/bin/less -N"
	autocrlf = input
  whitespace = trailing-space, space-before-tab, indent-with-non-tab
  logallrefupdates = true
  quotepath = false
[color]
  status = auto
  branch = auto
  interactive = auto
  diff = auto
	ui = true
[color "branch"]
  current = green reverse
  local = green
  remote = yellow
[color "diff"]
  meta = yellow
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow bold
  changed = red bold
  untracked = cyan
[web]
	browser = google-chrome
[rerere]
	enabled = 1
	autoupdate = 1
[help]
	autocorrect = 1
[credential]
	helper = osxkeychain
[merge]
	tool = emerge
  conflictstyle = diff3
[push]
	default = simple
[branch]
	autosetuprebase = always
[diff]
  renames = copy
  tool = ediff
  guitool = Kaleidoscope
[difftool]
    prompt = false
[difftool "Kaleidoscope"]
  cmd = ksdiff-wrapper git \"$LOCAL\" \"$REMOTE\"
##### TellApart specific ####
[include]
  path = .gitconfig_tellapart






